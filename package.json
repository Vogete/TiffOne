    },
    "keywords": [
        "tiff",
        "tif",
        "tiffone",
        "alternatiff"
    ],
    "homepage": "https://github.com/Vogete/TiffOne",
    "devDependencies": {
        "web-ext": "^4.0.0",
        "webextension-polyfill": "^0.6.0"
