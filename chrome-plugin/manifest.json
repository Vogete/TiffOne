{
    "manifest_version": 3,
    "name": "TiffOne",
    "version": "0.3.1",
    "description": "View embedded TIFF files and AlternaTIFF objects directly in your browser.",
    "author": "Zoltán Fraknói",
    "minimum_chrome_version": "51",
    "icons": {
        "16": "assets/TiffOne_logo_16px.png",
        "32": "assets/TiffOne_logo_32px.png",
        "48": "assets/TiffOne_logo_48px.png",
        "128": "assets/TiffOne_logo_128px.png"
    },
    "web_accessible_resources": [{
        "resources": [
            "templates/*",
            "fonts/*",
            "css/printing.css"
        ],
        "extension_ids": ["mhdcjcilknaincbghadamlbdfgnpcebg","beogakncemiochcinmcidmdnnbedieak"]
    }],
    "permissions": [
        "*://*/*",
        "file://*/*",
        "storage",
        "scripting",
        "activeTab"
    ],
    "action": {
        "default_title": "TIffOne",
        "default_popup": "html/popup.html"
    },
    "background": {
        "service_worker": "js/bg-loader.js"
    }
}
